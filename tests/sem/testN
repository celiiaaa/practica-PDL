// Funciones

function suma(a: character, b: character): int { // quedamos en que al sumar character + character se convierte en int?? No es tal cual lo que dice el enunciado
    return a + b;                                // tambien tengo mis dudas con bool y boolean 
}

function resta(a: int, b: float): int { // por alguna razon float y real lo pilla como error sintactico: [Syntax Error] At value False at line 7 at column 33
    return a - b;
}


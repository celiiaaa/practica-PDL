/* Mi primer programa */ 

// Declaración
let a;
let b;

// Asignación
a = 10;
b = a * a <= 200;

// Declaración y asignación
let c = 123;

// Re–declaración no permitida
let a;

// Re–asignación
c = 0xFF / (0b101 * (0702 - 1e-10));

// Declaración múltiple
let var1, var2;

// Asignación múltiple
var1, var2 = 10;

// Declaración y asignación múltiple
var1, var2 = 123;

// Declaración de tipo boleano
let var3 = tr;

// Error
let err1 = 5.0  * ((123 < 8) + ('s' == 'a'));


let var12 = (2+3+4+5) + ('a' + 'b' + 'c' + 'd') * 12;
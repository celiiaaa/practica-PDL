// TEST PARA LOS OBJETOS AJSON

// Creación de un objeto vacío
type Vacio = { };

// Redeclaración de un objeto vacío
type Vacio = { propiedad : int, };

// Creación de un objeto AJSON
type Objeto = { prop1 : character, prop2 : int, prop3 : boolean, };
type Cesta = { "cantidad" : int, otro : character };

type Tarea = { id : int, nombre : character, completada : boolean, };
type ListaTareas = { cantidad : int, "tarea1" : Tarea, "tarea2" : Tarea, "tarea3" : Tarea, };

// Creación de un objeto anidado
type Idioma = { nombre : character, hablantes : int };
type Pais = { nombre : character, capital : character, 
    dinero : { moneda : character, cantidad : int, }, 
    idiomas : { "idioma1" : Idioma, "idioma2" : Idioma, },
};

// Creación de un objeto que no existe
type Error = { mensaje : noExiste, };
type Respuesta = { "error" : Error, "resultado" : character, };

// Creación de un objeto con el mismo nombre que una variable
let VAR;
type VAR = { prop1 : int, }; 

// Creación de múltiples variables en una misma línea
let v1, v2 : Objeto, v3 : Vacio, v4;

// Asignación de un objeto a una variable
let mi_objeto : Objeto = { prop1 : 'h', prop2 : 42, prop3 : tr, };

// Asignación de un objeto a una variable con un tipo incorrecto
let mi_objeto2 : Objeto = { prop1 : 'h', prop2 : 42, prop3 : 42, };

// Asignación de un objeto a una variable con una propiedad incorrecta
let mi_objeto3 : Objeto = { prop1 : 'h', prop2 : 42, inventada : tr, };

// Asignación de un objeto a una variable con una propiedad de más
let mi_objeto4 : Objeto = { prop1 : 'h', prop2 : 42, prop3 : tr, prop4 : 123, };

// Acceso a las propiedades de un objeto
let propiedad1 = mi_objeto.prop1;

// Acceso a una propiedad inexistente de un objeto
let noexiste = mi_objeto.prop5;

// Asignación de una propiedad a un objeto
mi_objeto.prop1 = 'c';

// Asignación de una propiedad inexistente a un objeto
mi_objeto.prop6 = 3.14;

// Asignación de una propiedad con un tipo incorrecto a un objeto
mi_objeto.prop1 = 42;

// Asignación múltiple
mi_objeto.prop3, v1 = fl;


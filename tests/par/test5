// Bucle complejo
let x = 0;
type AJSON = { "variable1": int, variable2: int };
let obj: AJSON = { "variable1": 1, variable2: 2 };

while (x < 10) {
    let y = 0;
    while (y < 5) {
        let z = 0;
        while (z < 3) {
            if (z == 2) {
                x = x * 2;
                obj["variable1"] = obj["variable1"] + 1;
            } else {
                let w = 0;
                while (w < 2) {
                    if (w == 1) {
                        x = x + 3;
                        obj.variable2 = obj["variable2"] * 2;
                    } else {
                        x = x + 1;
                        // ERROR: La propiedad no existe
                        obj["a"] = obj["a"] * 3;
                    }
                    w = w + 1;
                }
            }
            z = z + 1;
        }
        y = y + 1;
    }
}
